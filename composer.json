{
    "name": "tijmenwierenga/guzzle-mock-handler",
    "description": "A better Guzzle mock handler",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tijmen Wierenga",
            "email": "t.wierenga@live.nl"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "guzzlehttp/promises": "^1.3",
        "phpstan/phpstan": "^0.11.19"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "guzzlehttp/guzzle": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "TijmenWierenga\\Guzzle\\Mocking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TijmenWierenga\\Tests\\Guzzle\\Mocking\\": "tests/"
        }
    }
}
